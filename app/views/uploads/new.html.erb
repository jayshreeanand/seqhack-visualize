<h2>Upload you files here</h2>

<%= form_for(@upload,  url: uploads_path(@upload), html: { method: :post }) do |f| %>

  <div class="field">
    <%= f.label :front_image, class: "sr-only"  %><br />
    <%= f.email_field :front_image, autofocus: true, class: "form-control", placeholder: "email address"  %>
  </div>


  <div class="actions">
    <%= f.submit "Upload", class: "btn btn-danger"  %>
  </div>
<% end %>

<%= render "devise/shared/links" %>
